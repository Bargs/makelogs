{
  "name": "makelogs",
  "version": "3.0.2",
  "description": "Simple log generator for testing kibana",
  "scripts": {
    "test": "npm run lint",
    "eslint": "eslint",
    "babel": "babel",
    "lint": "npm run eslint -- src --fix",
    "build": "rm -rf dest; npm run babel -- src -d dest -D -s true",
    "dev": "npm run build -- --watch",
    "prepublish": "npm run test; npm run build"
  },
  "bin": {
    "makelogs": "./dest/bin/makelogs"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/elastic/makelogs.git"
  },
  "keywords": [
    "elasticsearch",
    "kibana",
    "logstash",
    "test-data",
    "http",
    "traffic"
  ],
  "author": "Spencer Alger",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/spenceralger/makelogs/issues"
  },
  "homepage": "https://github.com/spenceralger/makelogs",
  "dependencies": {
    "async": "^1.4.2",
    "babel-polyfill": "^6.16.0",
    "bluebird": "^3.4.6",
    "elasticsearch": "^8.1.0",
    "inquirer": "^0.11.1",
    "lodash": "~2.4.1",
    "moment": "^2.10.6",
    "optimist": "~0.6.1",
    "progress": "^1.1.8",
    "through2": "^2.0.0",
    "update-notifier": "^0.5.0"
  },
  "devDependencies": {
    "@spalger/babel-presets": "^0.3.2",
    "@spalger/eslint-config-personal": "^0.6.2",
    "babel": "^6.5.2",
    "babel-cli": "^6.16.0",
    "babel-eslint": "^7.0.0",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-import-resolver-webpack": "^0.6.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "webpack": "^2.1.0-beta.25"
  }
}
